        <SQLTask:SqlTaskData
          SQLTask:Connection="{A6839ED7-CBE9-4AAA-A57A-AE6E4B46502F}"
          SQLTask:SqlStatementSource="DECLARE @CategoryID VARCHAR(24);&#xA;&#xA;DECLARE CategoryCursor CURSOR FOR&#xA;  SELECT CategoryID&#xA;  FROM Category;&#xA;&#xA;OPEN CategoryCursor;&#xA;FETCH NEXT FROM CategoryCursor INTO @CategoryID;&#xA;&#xA;WHILE @@FETCH_STATUS = 0&#xA;BEGIN&#xA;    DECLARE @FullName NVARCHAR(200);&#xA;&#xA;    WITH CategoryPath AS (&#xA;        SELECT CategoryID, name, parentId&#xA;        FROM Category C2&#xA;        WHERE CategoryID = @CategoryID&#xA;        UNION ALL&#xA;        SELECT C3.CategoryID, C3.name, C3.parentId&#xA;        FROM Category C3&#xA;        JOIN CategoryPath cp ON C3.CategoryID = cp.parentId&#xA;    )&#xA;    SELECT @FullName = STUFF((&#xA;        SELECT ' / ' + name&#xA;        FROM CategoryPath&#xA;        FOR XML PATH('')), 1, 3, '');&#xA;&#xA;    UPDATE Category&#xA;    SET FullName = @FullName&#xA;    WHERE CategoryID = @CategoryID;&#xA;&#xA;    FETCH NEXT FROM CategoryCursor INTO @CategoryID;&#xA;END;&#xA;&#xA;CLOSE CategoryCursor;&#xA;DEALLOCATE CategoryCursor;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
