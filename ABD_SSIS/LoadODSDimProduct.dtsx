      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{4402820C-74D5-4EB1-BD63-BFA48D87E4BF}"
          SQLTask:SqlStatementSource="DECLARE @CategoryID VARCHAR(24);&#xA;&#xA;DECLARE CategoryCursor CURSOR FOR&#xA;  SELECT CategoryID&#xA;  FROM dbo.DimCategorySt;&#xA;&#xA;OPEN CategoryCursor;&#xA;FETCH NEXT FROM CategoryCursor INTO @CategoryID;&#xA;&#xA;WHILE @@FETCH_STATUS = 0&#xA;BEGIN&#xA;    DECLARE @FullName NVARCHAR(200);&#xA;&#xA;    WITH CategoryPath AS (&#xA;        SELECT&#xA;            1 AS SortOrder, C1.CategoryID, C1.Name, C1.ParentID&#xA;        FROM dbo.DimCategorySt C1&#xA;        WHERE CategoryID = @CategoryID&#xA;        UNION ALL&#xA;        SELECT&#xA;            CP.SortOrder+1 AS SortOrder, C2.CategoryID, C2.Name, C2.ParentID&#xA;        FROM dbo.DimCategorySt C2&#xA;        JOIN CategoryPath CP&#xA;            ON C2.CategoryID = CP.ParentId&#xA;    )&#xA;    SELECT @FullName = STUFF((&#xA;        SELECT ' / ' + name&#xA;        FROM CategoryPath&#xA;        ORDER BY SortOrder DESC&#xA;        FOR XML PATH('')), 1, 3, '');&#xA;&#xA;    UPDATE DimCategorySt&#xA;    SET FullName = @FullName&#xA;    WHERE CategoryID = @CategoryID;&#xA;   &#xA;    FETCH NEXT FROM CategoryCursor INTO @CategoryID;&#xA;END;&#xA;&#xA;CLOSE CategoryCursor;&#xA;DEALLOCATE CategoryCursor;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
