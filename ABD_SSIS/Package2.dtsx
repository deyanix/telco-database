                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT   Client.ClientId, Client.FirstName, Client.SecondName, Client.LastName,
         CASE 
            WHEN Client.Sex = 'K' THEN CAST('Kobieta' AS NVARCHAR(12))
            WHEN Client.Sex = 'M' THEN CAST('Mężyczna' AS NVARCHAR(12))
         END AS Sex,
         Client.AddressId, Client.Email, Client.PhoneNumber, Address.Country,
         Address.City, Address.ZipCode, Address.Street, Address.BuildingNumber, Address.ApartmentNumber
